<?xml version="1.0" encoding="UTF-8"?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        https://symfony.com/schema/dic/services/services-1.0.xsd">

    <framework:config xmlns:framework="http://symfony.com/schema/dic/symfony">
        <framework:messenger>
            <framework:routing message-class="KdotPlayground\Message\KdotUpsertMessage">
                <framework:sender service="messenger.transport.async"/>
            </framework:routing>
        </framework:messenger>
    </framework:config>

</container>
